<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.orm.test.query">

    <class name="org.hibernate.orm.test.query.ResultMappingAssociationTest$ItemInventory">
        <composite-id>
            <key-many-to-one name="account" column="account_id"/>
            <key-many-to-one name="item" column="item_id"/>
        </composite-id>
        <property name="quantity"/>
    </class>
</hibernate-mapping>
