<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.test.instrument.domain">
    <class name="AEntity2" table="A">
        <id name="oid"/>
        <property name="a"/>
    </class>

    <class name="BEntity2" table="B">
        <id name="oid"/>
        <property name="b1"/>
        <property name="b2"/>
    </class>

    <class name="CEntity2" table="C">
        <id name="oid"/>
        <property name="c1"/>
        <property name="c2"/>
        <property name="c3"/>
        <property name="c4"/>
    </class>

    <class name="DEntity2" table="D">
        <id name="oid"/>
        <property name="d"/>
        <property name="blob" lazy="true"/>
        <many-to-one name="a" lazy="proxy"/>
        <many-to-one name="c" lazy="proxy"/>
        <set name="bs" lazy="true">
            <key column="b_oid"/>
            <many-to-many class="BEntity2"/>
        </set>
        <one-to-one name="e" lazy="proxy" property-ref="d"/>
    </class>

    <class name="EEntity2" table="E">
        <id name="oid"/>
        <property name="e1"/>
        <property name="e2"/>
        <many-to-one name="d" lazy="proxy" unique="true"/>
    </class>
</hibernate-mapping>